{
    "name": "acr/roles",
    "description": ":description",
    "authors": [
        {
            "name": "AydÄ±n ACAR",
            "email": "acarbey15@gmail.com",
            "homepage": "https://aydinacar.net"
        }
    ],
    "homepage": "https://github.com/acr/roles",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/acr/roles"
        }
    ],
    "keywords": ["Laravel", "roles"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "acr\\roles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "acr\\roles\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "acr\\roles\\rolesServiceProvider"
            ],
            "aliases": {
                "roles": "acr\\roles\\Facades\\roles"
            }
        }
    }
}
